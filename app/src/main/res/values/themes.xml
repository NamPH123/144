<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.St144_icon_changer" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>

    <style name="Theme.St144_icon_changer" parent="Base.Theme.St144_icon_changer" />
    <style name="BaseDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <style name="ShapeAppearanceOverlay.App.CornerSize50Percents" parent="">
        <item name="cornerSize">50%</item>
    </style>
    <style name="ShapeAppearanceOverlay.App.CornerSize10Percents" parent="">
        <item name="cornerSize">10dp</item>
    </style>
    <style name="ShapeAppearanceOverlay.App.CornerSize7Percents" parent="">
        <item name="cornerSize">7dp</item>
    </style>
    <style name="ShapeAppearanceOverlay.App.CornerSize4Percents" parent="">
        <item name="cornerSize">4dp</item>
    </style>
    <style name="App.Custom.Indicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:color">#00000000</item>
    </style>
    <style name="BottomNavTextActive" parent="TextAppearance.MaterialComponents.Body1">
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="BottomNavTextInactive" parent="TextAppearance.MaterialComponents.Body2">
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal</item>
    </style>
</resources>